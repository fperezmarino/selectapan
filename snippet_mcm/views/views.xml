<odoo>
  <data>

<template id="snippet_mcm_form" inherit_id="website.snippets"
              name="voto MCM ">
        <xpath expr="//div[@id='snippet_structure']/div[hasclass('o_panel_body')]"
               position="inside">
            <t t-snippet="snippet_mcm.snippet_mcmform"/>
        </xpath>
    </template> 
  <template id="snippet_mcmform" name="voto MCM">
  <section class="s_product_catalog oe_custom_bg o_colored_level o_bg_img_opt_repeat pt112 pb128 oe_img_bg o_bg_img_center" style="background-image: url(&quot;/web/image/881-732f14bc/elecciones-regionales-barinas-venezuelajpg%20%281%29.jpg&quot;); background-size: 1903px !important;" data-vcss="001" data-snippet="s_product_catalog" data-name="Pricelist" data-original-title="" title="" aria-describedby="tooltip281886" data-original-id="880" data-original-src="/web/image/880-cec9d673/elecciones-regionales-barinas-venezuelajpg%20%281%29.jpg" data-mimetype="image/jpeg" data-resize-width="855" data-gl-filter="blur">
        <div class="container" bis_skin_checked="1">
          <h2 class="o_default_snippet_text">
            <br/>
          </h2>
          <div class="row" bis_skin_checked="1">
            <div class="col-lg-6 o_colored_level pb0 pt88" style="" bis_skin_checked="1">
              <h3>
                <font class="text-o-color-4">¿Votará usted por María Corina Machado en las elecciones presidenciales del 22/10/2024, para ejercer el cargo de Presidente de la República Bolivariana de Venezuela?</font>
                <br/>
              </h3>
              <ul class="list-unstyled my-3">
                <li class="s_product_catalog_dish" data-name="Product">
                  <br/>
                </li>
                <li class="s_product_catalog_dish" data-name="Product">
                  <br/>
                </li>
              </ul>
            </div>
            <div class="pt16 pb16 o_colored_level rounded bg-black-50 shadow col-lg-6" style="border-radius: 40px !important; box-shadow: rgba(0, 0, 0, 0.15) 0px 8px 100px 10px !important;" bis_skin_checked="1">
              <h3>Tu intención de voto es Importante</h3>
              <section class="s_website_form pt16 pb16" data-vcss="001" data-snippet="s_website_form" data-name="Form" data-original-title="" title="" aria-describedby="tooltip765195">
                <div class="container" bis_skin_checked="1">
                  <form action="/website/form/" method="post" enctype="multipart/form-data" class="o_mark_required" data-mark="*" data-pre-fill="true" data-success-mode="redirect" data-success-page="/contactus-thank-you" data-model_name="mail.mail">
                    <div class="s_website_form_rows row s_col_no_bgcolor" bis_skin_checked="1">
                      <div data-visibility-condition="" data-visibility-between="" class="form-group s_website_form_field col-12 col-lg-6 s_website_form_custom s_website_form_required" data-type="char" data-name="Field" bis_skin_checked="1">
                        <label class="s_website_form_label" style="width: 200px" for="nombresm">
                          <span class="s_website_form_label_content">Nombre</span>
                          <span class="s_website_form_mark">               *</span>
                        </label>
                        <input type="text" class="form-control s_website_form_input" name="Nombre" required="1" placeholder="" id="nombresm" data-fill-with="name"/>
                      </div>
                      <div data-visibility-condition="" data-visibility-between="" class="form-group s_website_form_field col-12 col-lg-6 s_website_form_custom" data-type="integer" data-name="Field" bis_skin_checked="1">
                        <label class="s_website_form_label " style="width: 200px" for="cedulasm">
                          <span class="s_website_form_label_content">Cedula</span>
                        </label>
                        <input type="number" class="form-control s_website_form_input" name="Cedula" step="1" placeholder="" id="cedulasm" data-fill-with="phone"/>
                      </div>
                      <div data-visibility-condition="" data-visibility-between="" class="form-group s_website_form_field col-12 col-lg-6 s_website_form_model_required" data-type="char" data-name="Field" bis_skin_checked="1">
                        <label class="s_website_form_label " style="width: 200px" for="correosm">
                          <span class="s_website_form_label_content">Correo</span>
                          <span class="s_website_form_mark">                    *</span>
                        </label>
                        <input type="text" class="form-control s_website_form_input" name="email_from" required="true" placeholder="" id="correosm" data-fill-with="email"/>
                      </div>
                      <div data-visibility-condition="" data-visibility-between="" class="form-group s_website_form_field col-12 col-lg-6 s_website_form_model_required o_we_force_no_transition" data-type="char" data-name="Field" bis_skin_checked="1">
                        <label class="s_website_form_label " style="width: 200px" for="centrosm">
                          <span class="s_website_form_label_content">Centro de Votación</span>
                          <span class="s_website_form_mark">              *</span>
                        </label>
                        <input type="text" class="form-control s_website_form_input" name="subject" required="true" value="" placeholder="" id="centrosm" data-fill-with="undefined"/>
                      </div>
                      <div class="form-group s_website_form_field col-12    s_website_form_dnone" data-name="Field" bis_skin_checked="1">
                        <div class="row s_col_no_resize s_col_no_bgcolor" bis_skin_checked="1">
                          <label class="col-form-label col-sm-auto s_website_form_label " style="width: 200px">
                            <span class="s_website_form_label_content"/>
                          </label>
                          <div class="col-sm" bis_skin_checked="1">
                            <input type="hidden" class="form-control s_website_form_input" name="email_to" value="info@yourcompany.example.com"/>
                            <div class="s_website_form_field_description small form-text text-muted" bis_skin_checked="1">
            Separar las direcciones de correo electrónico con una coma.
        </div>
                          </div>
                        </div>
                      </div>
                      <div class="form-group col-12" >
                        <div style="width: 200px;" class="s_website_form_label"/>
                        <a href="#" role="button" class="btn btn-primary btn-lg " id="enviarsm">Enviar</a>
                        <span id=""/>
                      </div>
                    </div>
                  </form>
                </div>
              </section>
              <ul class="list-unstyled my-3">
                <li class="s_product_catalog_dish" data-name="Product">
                  <br/>
                </li>
              </ul>
            </div>
          </div>
        </div>
      </section>
      <div class="modal fade bd-example-modal-lg" tabindex="-1" role="dialog" id="modal_confirmacion">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Datos del Votante</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                       
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row">
                        <div class="form-group col-12" id="block_votante"></div> 
                    </div>
                  
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                </div>
            </div>
        </div>
    </div>
  </template>
    
      



  
  </data>
</odoo>